body{
	color:#555;
	font-family: verdana,geneva,arial,helvetica, sans-serif;
	line-height:1.5em;
	margin:0;
	padding:0;
	text-align:left;
	font-size:100%;
	
}

#content{
	font-size:1.2em;
	float:left; 
	padding:40px;
}

 #footer{
	/*background:#333;
	border-top:1px solid #777;
	color:#888; */
	clear:both;
	font-size:0.9em;
	padding-top:75px;
}

/*************************************
 +Hn and p
 *************************************/
h1, h2 {
	font-weight:normal;
	letter-spacing: -1px;
	margin:0;
}
#header h1{
	font-size:3em;
	margin:0;
	margin-right:1em;
}
#header h1 a{
	color:#E87830;
	text-decoration:none;
}
#header h1 a:hover{
	color:#CC0000;
}
#header p{
	color:#aaa;
	font-size:1.1em;
	margin:0;
	margin-top:-0.5em;
}
#header img { border: none;
}
