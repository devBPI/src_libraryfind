body { background-color: #ffdfad; margin: 0px; padding: 0px; } 

h1 { color: #000000; font-size: 18px; font-family: verdana,geneva,arial,helvetica, sans-serif; font-weight: 600; margin-bottom: 30px; }

h2 { color: #000000; font-size: 15px; font-family: verdana,geneva,arial,helvetica, sans-serif; font-weight: 600; margin-top: 30px; margin-bottom: -4px; }

h3 { color: #000000; font-weight: 700; font-size: 11px;  font-family: verdana,geneva,arial,helvetica, sans-serif; margin-bottom: -12px; }

b, strong { font-weight: 700; font-family: verdana,geneva,arial,helvetica, sans-serif; }

i, em { font-family: verdana,geneva,arial,helvetica, sans-serif; }

p { color: #000000; font-size: 11px; font-family: verdana,geneva,arial,helvetica, sans-serif; line-height: 19px; margin-top: 19px; margin-bottom: 15px; }

table { margin-bottom: 15px; margin-top: 15px; }

td { color: #000000; font-size: 11px; font-family: verdana,geneva,arial,helvetica, sans-serif; }

ul { list-style-type: none; font-size: 11px; font-family: verdana,geneva,arial,helvetica, sans-serif; line-height: 19px; margin-top: 19px; margin-left: 16px; }

ul ul { line-height: 19px; margin-top: 5px; }

ul li { list-style-type: none; font-family: verdana,geneva,arial,helvetica, sans-serif; line-height: 16px; margin-top: 1px; margin-bottom:8px; }

ol { list-style-type: decimal; font-size: 11px; font-family: verdana,geneva,arial,helvetica, sans-serif; line-height: 19px; margin-top: 19px; }

ol ol { list-style-type: lower-alpha; line-height: 19px; margin-top: 5px; }

ol ol ol { list-style-type: decimal; line-height: 19px; margin-top: 5px; }

li a { text-decoration: none; }

pre { font-family: verdana,geneva,arial,helvetica, sans-serif; }

a:link { color: #0000c7; font-family: verdana,geneva,arial,helvetica,sans-serif; text-decoration: none; }
