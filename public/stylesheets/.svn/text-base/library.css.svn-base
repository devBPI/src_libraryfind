a.contents:hover { color: #0000c7; font-family: verdana,geneva,arial,helvetica, sans-serif; text-decoration: none; }

a.index:link { color: #000000; font-size: 11px; font-family: arial,helvetica, sans-serif; line-height: 16px; margin-top: 2px; margin-bottom: 1px; text-decoration: none; }
a.index:visited { color: #000000; font-size: 11px; font-family: arial,helvetica, sans-serif; line-height: 16px; margin-top: 2px; margin-bottom: 1px; text-decoration: none; }
