a.indexw2:link { color: #ffffff; font-size: 12px; font-weight: 500; font-family: arial,helvetica, sans-serif; line-height: 19px; margin-top: 2px; margin-bottom: 1px; text-decoration: none; }
a.indexw2:visited { color: #ffffff; font-size: 12px; font-weight: 500; font-family: arial,helvetica, sans-serif; line-height: 19px; margin-top: 2px; margin-bottom: 1px; text-decoration: none; }
