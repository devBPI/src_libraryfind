a.onav:visited { font-size: 10px; color: #ffffff; font-family: arial,helvetica,sans-serif; }
a.onav:hover { font-size: 10px; color: #DDB420; font-family: arial,helvetica,sans-serif; }

a.vr:link { font-size: 9px; color: #0000c7; font-family: arial,helvetica,sans-serif; text-decoration: underline; }
a.vr:visited { font-size: 9px; color: #660066; font-family: arial,helvetica,sans-serif; text-decoration: underline; }
